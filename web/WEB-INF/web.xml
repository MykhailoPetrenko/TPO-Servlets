<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <context-param>
        <param-name>presentationServ</param-name>
        <param-value>/presentation</param-value>
    </context-param>

    <context-param>
        <param-name>resBundleServ</param-name>
        <param-value>/resBundleServ</param-value>
    </context-param>

    <context-param>
        <param-name>getParamsServ</param-name>
        <param-value>/getparams</param-value>
    </context-param>

    <context-param>
        <param-name>commandClassName</param-name>
        <param-value>FindCommand</param-value>
    </context-param>

    <servlet>
        <servlet-name>ErrorHandler</servlet-name>
        <servlet-class>ErrorHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ErrorHandler</servlet-name>
        <url-pattern>/errorhandler</url-pattern>
    </servlet-mapping>

    <context-param>
        <param-name>dbName</param-name>
        <param-value>jdbc/ksidb</param-value>
    </context-param>


</web-app>